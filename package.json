{
  "name": "dragon-quest-server",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel ./src -d dist",
    "server": "node -r dotenv/config dist/index.js",
    "start": "npm run build && node -r dotenv/config dist/index.js",
    "watch": "nodemon --watch --exec npm start",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:undo:all": "npx sequelize-cli db:seed:all"
  },
  "author": "Mateusz Bieli≈Ñski",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.1",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "mysql2": "^2.3.3",
    "sequelize": "^6.16.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.0",
    "@babel/core": "^7.17.2",
    "@babel/preset-env": "^7.16.11",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.2",
    "nodemon": "^2.0.15",
    "sequelize-cli": "^6.4.1"
  }
